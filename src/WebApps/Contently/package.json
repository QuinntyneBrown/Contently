{
  "name": "contently",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "watch:libs": "npm-run-all --parallel watch:content-core watch:identity-core watch:dashboard-core",
    "watch:content-core": "ng build @content/core --watch",
    "watch:identity-core": "ng build @identity/core --watch",
    "watch:global": "npm-run-all --parallel watch:global-styles watch:global-components",
    "watch:global-styles": "ng build @global/styles --watch",
    "watch:global-components": "ng build @global/components --watch",
    "watch:dashboard-core": "ng build @dashboard/core --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.1.4",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.1.4",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@microsoft/signalr": "^7.0.2",
    "@ngrx/component": "^15.3.0",
    "@ngrx/component-store": "^15.2.1",
    "ckeditor4-angular": "^3.2.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^15.0.0",
    "@angular-devkit/build-angular": "^15.0.5",
    "@angular/cli": " ",
    "@angular/compiler-cli": "^15.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@types/jest": "^29.4.0",
    "concurrently": "^7.6.0",
    "jasmine-core": "~4.5.0",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.6",
    "ng-packagr": "^15.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.8.2"
  }
}